    "@apps/website": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/@apps/website",
      "sourceRoot": "projects/@apps/website/src",
      "prefix": "website",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "@firebase/app",
              "firebase/app"
            ],
            "outputPath": "dist/@apps/website",
            "index": "projects/@apps/website/src/index.html",
            "main": "projects/@apps/website/src/main.ts",
            "polyfills": "projects/@apps/website/src/polyfills.ts",
            "tsConfig": "projects/@apps/website/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/@apps/website/src/favicon.ico",
              "projects/@apps/website/src/assets"
            ],
            "styles": [
              "projects/@apps/website/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/@apps/website/src/environments/environment.ts",
                  "with": "projects/@apps/website/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "@apps/website:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "@apps/website:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "@apps/website:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/@apps/website/src/test.ts",
            "polyfills": "projects/@apps/website/src/polyfills.ts",
            "tsConfig": "projects/@apps/website/tsconfig.spec.json",
            "karmaConfig": "projects/@apps/website/karma.conf.js",
            "assets": [
              "projects/@apps/website/src/favicon.ico",
              "projects/@apps/website/src/assets"
            ],
            "styles": [
              "projects/@apps/website/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/@apps/website/tsconfig.app.json",
              "projects/@apps/website/tsconfig.spec.json",
              "projects/@apps/website/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/@apps/website/e2e/protractor.conf.js",
            "devServerTarget": "@apps/website:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "@apps/website:serve:production"
            }
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {}
        }
      }
    },
